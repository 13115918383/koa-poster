            if ($(el).text().indexOf('遥网遭黑客攻击') > 0) {
              $(el).css('order', '999');
            }
          });
        }
      }, 20);
      // noinspection UnnecessaryReturnStatementJS
      return;
    }

    if (isPathMatch('jx2xz.xoyo.com', ['/index1.html'])) {
      setInterval(function () {
        if ($) {
          var $list = $('.article_list').eq(0).find('div');

          $list.css('display', 'flex');
          $list.css('flex-direction', 'column');

          Array.prototype.find.call($list.find('li'), function (el) {
            if ($(el).text().indexOf('遥网遭黑客攻击') > 0) {
              $(el).css('order', '999');
            }
          });
        }
      }, 20);
      // noinspection UnnecessaryReturnStatementJS
      return;
    }
  } catch(e){}
}());
